  <step>
    <name>sql-script - batch_digest-for-parent-batch</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>dw-biologia</connection>
    <execute_each_row>N</execute_each_row>
    <single_statement>N</single_statement>
    <replace_variables>Y</replace_variables>
    <quoteString>N</quoteString>
    <sql>
update batch pb
join (select batch_parent_id, 
			sum(rows_inserted) as total_rows_inserted, 
			sum(rows_updated) as total_rows_updated,
			sum(rows_deleted) as total_rows_deleted
		from batch
		where batch_parent_id = ${batch_id}) as cb on cb.batch_parent_id = pb.batch_id
set
	end_on = now(),
	status = '${status}',
	pb.rows_inserted = cb.total_rows_inserted,
	pb.rows_updated = cb.total_rows_updated,
	pb.rows_deleted = cb.total_rows_deleted		
where pb.batch_id = ${batch_id};</sql>
